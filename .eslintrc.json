{
  "env": {
    "node": true,
    "commonjs": true
  },
  "plugins": ["node"],
  "extends": ["eslint:recommended", "plugin:node/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "rules": {
    "node/no-missing-require": "error",
    "node/no-unsupported-features/es-syntax": "off"
  }
}